<configuration>
  <packageSources>
    <clear />
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" protocolVersion="3" />
    <add key="sec-cyberres-nuget-dev"
         value="https://svsartifactory.swinfra.net/artifactory/api/nuget/sec-cyberres-nuget-dev" />
    <add key="sec-cyberres-nuget-prerelease"
         value="https://svsartifactory.swinfra.net/artifactory/api/nuget/sec-cyberres-nuget-prerelease" />
    <add key="sepg-opensource-nuget-prerelease"
         value="https://svsartifactory.swinfra.net/artifactory/api/nuget/sepg-opensource-nuget-prerelease" />
    <add key="local-packages" value="." />
  </packageSources>
  <packageSourceCredentials>
    <sec-cyberres-nuget-dev>
      <add key="Username" value="%SEPG_NUGET_USERNAME%" />
      <add key="ClearTextPassword" value="%SEPG_NUGET_PASSWORD%" />
    </sec-cyberres-nuget-dev>
    <sec-cyberres-nuget-prerelease>
      <add key="Username" value="%SEPG_NUGET_USERNAME%" />
      <add key="ClearTextPassword" value="%SEPG_NUGET_PASSWORD%" />
    </sec-cyberres-nuget-prerelease>
  </packageSourceCredentials>
  <packageSourceMapping>
    <packageSource key="local-packages">
      <package pattern="MicroFocus.FAS.AdapterSdkSchema*" />
    </packageSource>
  </packageSourceMapping>
</configuration>
