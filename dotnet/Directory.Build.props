<Project>
  <PropertyGroup>
    <BaseIntermediateOutputPath>$(MSBuildThisFileDirectory)artifacts/$(MSBuildProjectName)/obj/</BaseIntermediateOutputPath>
    <BaseOutputPath>$(MSBuildThisFileDirectory)artifacts/$(MSBuildProjectName)/bin/</BaseOutputPath>
    <Company>Micro Focus</Company>
    <Copyright>$(
        [System.Text.RegularExpressions.Regex]::Match($([System.IO.File]::ReadAllText($(MSBuildThisFileFullPath))), `Copyright.*`)
    )</Copyright>
    <PackageProjectUrl>https://github.com/FileAnalysisSuite/adaptersdk-schema</PackageProjectUrl>
    <RepositoryUrl>https://github.com/FileAnalysisSuite/adaptersdk-schema.git</RepositoryUrl>
  </PropertyGroup>
</Project>
